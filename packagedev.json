{
  "name": "n-http-2",
  "version": "1.0.7",
  "description": "Nodes ApS Angular Http Library.",
  "repository": {
    "url": "https://github.com/nodes-frontend/n-http-2"
  },
  "license": "MIT",
  "scripts": {
    "build": "ngc --p tsconfig-build.json",
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "docs": "typedoc --tsconfig tsconfig.typedoc.json --readme README.md --name NHttp2 --excludeExternals --out docs && node ./post-docs",
    "server": "webpack-dev-server --inline --progress --port 8080",
    "server:docs": "lite-server -c lite-server.config.js",
    "start": "npm run server",
    "start:hmr": "npm run server -- --hot",
    "start:docs": "npm run server:docs",
    "watch": "webpack --watch --progress --profile"
  },
  "dependencies": {
    "@angular/common": "2.4.6",
    "@angular/compiler": "2.4.6",
    "@angular/core": "2.4.6",
    "@angular/forms": "2.4.6",
    "@angular/http": "2.4.6",
    "@angular/platform-browser": "2.4.6",
    "@angular/platform-browser-dynamic": "2.4.6",
    "@angular/router": "^3.4.6",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler": "^2.4.6",
    "@angular/compiler-cli": "^2.4.6",
    "@angular/platform-server": "^2.4.6",
    "@angularclass/hmr": "^1.0.1",
    "@angularclass/hmr-loader": "^3.0.2",
    "@types/core-js": "^0.9.0",
    "@types/jasmine": "^2.2.29",
    "@types/node": "^6.0.38",
    "@types/selenium-webdriver": "2.53.33",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.3.2",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "codelyzer": "1.0.0-beta.3",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.8.1",
    "json-loader": "^0.5.3",
    "lite-server": "^2.2.2",
    "node-sass": "^3.4.2",
    "null-loader": "0.1.1",
    "postcss-loader": "^1.1.0",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^4.0.0",
    "shelljs": "^0.7.0",
    "style-loader": "^0.13.0",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.4.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.5.1",
    "typescript": "^2.1.5",
    "url-loader": "^0.5.6",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.9"
  }
}
